 <RecipeDef ParentName="AM_LightMechanoidRecipe">
    <defName>AM_DaggersnoutRecipe</defName>
    <label>gestate daggersnout</label>
    <description>Gestate a daggersnout mechanoid.</description>
    <ingredients Inherit="False">
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>SubcoreBasic</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <AM_Daggersnout>1</AM_Daggersnout>
    </products>
    <descriptionHyperlinks>
      <ThingDef>AM_Daggersnout</ThingDef>
    </descriptionHyperlinks>
  </RecipeDef>

